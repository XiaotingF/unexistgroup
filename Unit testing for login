 /** @test */
    public function a_user_can_log_in()
    {
        $user = factory(App\User::class)->create([
             'email' => '1@123.com', 
             'password' => bcrypt('123456')
        ]);

        $this->visit(route('login'))
            ->type($user->email, 'email')
            ->type('123456', 'password')
            ->press('Login')
            ->see('Successfully logged in')
            ->onPage('/profile');
    }
